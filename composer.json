{
    "name": "hexlet/code",
    "autoload": {
        "files": [
            "src/Differ.php",
            "src/Parsers.php",
            "src/Formatters.php",
            "src/Utilits.php",
            "src/RequireAutoload.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Json.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
          "Hexlet\\Code\\Tests\\": "tests"
        }
      },
    "authors": [
        {
            "name": "Konstantin",
            "email": "konst.kazan@gmail.com"
        }
    ],
    "require": {
        "php" : ">=8.2",
        "docopt/docopt": "^1.0",
        "tightenco/collect": "^9.33",
        "nesbot/carbon": "^2.64",
        "symfony/string": "^6.2",
        "phpstan/phpstan": "^1.9.2",
        "symfony/yaml": "^7.0",
        "lstrojny/functional-php": "^1.17"
      },
      "require-dev": {
        "psy/psysh": "@stable",
        "phpstan/phpstan-phpunit": "^1.2.2",
        "phpstan/extension-installer": "^1.2",
        "symfony/var-dumper": "^6.2.1",
        "phpunit/phpunit": "^9.5.26",
        "squizlabs/php_codesniffer": "^3.7.1"
      },
    "license": "MIT",
    "bin": [
        "bin/gendiff"
      ],
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
